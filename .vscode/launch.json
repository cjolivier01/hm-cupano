  "version": "0.2.0",
  "configurations": [
    {
      "name": "----------------- hm-cupano -----------------",
    },
    {
      //
      // Python: Current File
      //
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceRoot}/build:${workspaceRoot}/src:${workspaceRoot}/external/MOTChallengeEvalKit"
      }
    },
    {
      //
      // Python: Create Control Points
      //
      "name": "Python: create_control_points.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/create_control_points.py",
      "args": [
        //"--game-id=mlk-heat-1",
        //"--left=${env:HOME}/Videos/mlk-heat-1/GX010097.MP4",
        //"--right=${env:HOME}/Videos/mlk-heat-1/GX010016.MP4",
        // "${env:HOME}/Videos/stitch-fix/hm_project.pto",
        "--left=${env:HOME}/Videos/fremont/left.mp4",
        "--right=${env:HOME}/Videos/fremont/right.mp4",
        "--synchronize-only",
        // "--lfo=0.735",
        // "--rfo=0",
        // "--scale=0.7",
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceRoot}/build:${workspaceRoot}/src:${workspaceRoot}/external/MOTChallengeEvalKit"
      }
    },
    {
      "name": "(LLDB) cudaBlend",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bazel-bin/tests/test_cuda_blend",
      "args": [
        //"--game-id=stitch-fix",
        "--game-id=mlk-heat-1",
        //"--game-id=ev-tv-1a",
        // "--game-id=pdp",
        //"--show",
        "--adjust=1",
        //"--levels=0",
        "--levels=6",
        "--perf",
        //"--output=${workspaceRoot}/output.png",
      ],
      "sourceMap": {
        "/proc/self/cwd": "${workspaceFolder}",
        "~/src": "${env:HOME}/src",
        "external/deepstream/sources": "${env:HOME}/src/hstream/external/deepstream/sources",
        "home/colivier/src/jetson-utils": "${workspaceFolder}/src/external/jetson-utils",
      },
      "env": {
        "DISPLAY": ":1",
        "CUDA_LAUNCH_BLOCKING": "1",
      },
      //"stopOnEntry": true,
      "preLaunchTask": "Build All",
    },
    {
      "name": "(CUDA) cudaBlend",
      "type": "cuda-gdb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "debuggerPath": "/usr/local/cuda/bin/cuda-gdb",
      "program": "${workspaceFolder}/bazel-bin/tests/test_cuda_blend",
      "args": "--game-id=pdp",
      "initCommands": [
        "directory ${workspaceFolder}",
      ],
      "preLaunchTask": "Build All",
      "environment": [
        {
          "name": "CUDA_LAUNCH_BLOCKING",
          "value": "1"
        }
      ],
    },
    {
      "name": "(LLDB) Stabilization",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bazel-bin/src/stable/stabilization",
      "args": [
        "/mnt/home/colivier-local/src/hstream/bouncy.mp4",
        "unbouncy.mp4",
      ],
      "sourceMap": {
        "/proc/self/cwd": "${workspaceFolder}",
        "~/src": "${env:HOME}/src",
        "external/deepstream/sources": "${env:HOME}/src/hstream/external/deepstream/sources",
        "home/colivier/src/jetson-utils": "${workspaceFolder}/src/external/jetson-utils",
      },
      "env": {
        "DISPLAY": ":1",
        "CUDA_LAUNCH_BLOCKING": "1",
      },
      //"stopOnEntry": true,
      "preLaunchTask": "Build All",
    },
  ]
}
