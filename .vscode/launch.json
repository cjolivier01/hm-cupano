{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  //
  "version": "0.2.0",
  "configurations": [
    {
      "name": "----------------- jetson-utils -----------------",
    },
    {
      //
      // Python: Current File
      //
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceRoot}/build:${workspaceRoot}/src:${workspaceRoot}/external/MOTChallengeEvalKit"
      }
    },
    {
      "name": "gl-display-test",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/display/gl-display-test/gl_display_test",
      "args": [],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "JU Build All",
    },
    {
      "name": "(LLDB) cudaBlend",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bazel-bin/tests/test_cuda_blend",
      "args": [
        "stitch-fix",
        //"mlk-heat-1",
        //"pdp",
        "${workspaceRoot}/output.png",
      ],
      "sourceMap": {
        "/proc/self/cwd": "${workspaceFolder}",
        "~/src": "${env:HOME}/src",
        "external/deepstream/sources": "${env:HOME}/src/hstream/external/deepstream/sources",
        "home/colivier/src/jetson-utils": "${workspaceFolder}/src/external/jetson-utils",
      },
      //"stopOnEntry": true,
      "env": {
        "DISPLAY": ":1",
        "CUDA_LAUNCH_BLOCKING": "1",
      },
      "preLaunchTask": "JU Build All",
    },
    {
      "name": "(CUDA) cudaBlend",
      "type": "cuda-gdb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "debuggerPath": "/usr/local/cuda/bin/cuda-gdb",
      "program": "${workspaceFolder}/bazel-bin/tests/test_cuda_blend",
      "args": "stitch-fix ${workspaceRoot}/output.png",
      "initCommands": [
        "directory ${workspaceFolder}",
      ],
      "preLaunchTask": "JU Build All",
    },
  ]
}
